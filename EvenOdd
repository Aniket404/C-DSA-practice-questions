// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

bool isEven(int a){
    if(a&1){
        return 0;
    }
    else{
        return 1;
    }
}

int main() {
    // Write C++ code here
    int num;
    cin>>num;
    if(isEven(num)){
        cout<<"num is even"<<endl;
    }
    else{
        cout<<"num is odd";
    }

    //return 0;
}
